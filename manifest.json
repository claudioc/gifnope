{
  "manifest_version": 2,

  "name": "Stops your annoying gifs",
  "description": "This extension allows you to stop annoying gifs in the current page",
  "version": "1.0",

  "icons": { "16": "icon16.png",
             "48": "icon48.png",
             "128": "icon128.png" },

  "permissions": [
      "contextMenus"
  ],

  "content_scripts": [{
      "matches": ["*://*/*"],
      "js":      ["content.js"]
  }],

 "background": {
      "scripts": ["main.js"]
  }
}
